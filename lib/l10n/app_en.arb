  "@_NAME_NUMBER_50": {},
  "name_original_50": "الشهيد",
  "name_transcription_50": "Ash-Shaheed",
  "name_translate_50": "The All Observing Witness",
  "name_description_50": "The Witness, The One who nothing is absent from Him. The All- and Ever Witnessing",
  "@_NAME_NUMBER_51": {},
  "name_original_51": "الحق",
  "name_transcription_51": "Al-Haqq",
  "name_translate_51": "The Embodiment of Truth",
  "name_description_51": "The Truth, The True, The One who truly exists. The Absolute Truth",
  "@_NAME_NUMBER_52": {},
  "name_original_52": "الوكيل",
  "name_transcription_52": "Al-Wakeel",
  "name_translate_52": "The Universal Trustee",
  "name_description_52": "The Trustee, The Disposer of Affairs, The One who gives the satisfaction and is relied upon",
  "@_NAME_NUMBER_53": {},
  "name_original_53": "القوى",
  "name_transcription_53": "Al-Qawiyy",
  "name_translate_53": "The Strong One",
  "name_description_53": "The Most Strong, The Strong, The One with the complete Power",
  "@_NAME_NUMBER_54": {},
  "name_original_54": "المتين",
  "name_transcription_54": "Al-Mateen",
  "name_translate_54": "The Firm One",
  "name_description_54": "The Steadfast. The One with extreme Power which is un-interrupted and He does not get tired",
  "@_NAME_NUMBER_55": {},
  "name_original_55": "الولى",
  "name_transcription_55": "Al-Waliyy",
  "name_translate_55": "The Protecting Associate",
  "name_description_55": "The Protecting Friend, The Supporter. Allah is always there for those who worship and love Him sincerely",
  "@_NAME_NUMBER_56": {},
  "name_original_56": "الحميد",
  "name_transcription_56": "Al-Hameed",
  "name_translate_56": "The Sole-Laudable One",
  "name_description_56": "The Praiseworthy, The praised One who deserves to be praised",
  "@_NAME_NUMBER_57": {},
  "name_original_57": "المحصى",
  "name_transcription_57": "Al-Muhsee",
  "name_translate_57": "The All-Enumerating One",
  "name_description_57": "The Counter, The Reckoner, The One who the count of things are known to him",
  "@_NAME_NUMBER_58": {},
  "name_original_58": "المبدئ",
