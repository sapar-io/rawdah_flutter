  "name_translate_58": "The Originator",
  "name_description_58": "The Initiator. The One who started the human being. That is, He created him",
  "@_NAME_NUMBER_59": {},
  "name_original_59": "المعيد",
  "name_transcription_59": "Al-Mu'id",
  "name_translate_59": "The Restorer",
  "name_description_59": "The Reinstater. The Reproducer, The One who brings back the creatures after death",
