  "@_NAME_NUMBER_23": {},
  "name_original_23": "ٱلرَّافِعُ",
  "name_transcription_23": "Ar-Raafi'",
  "name_translate_23": "The Elevating One",
  "name_description_23": "The Exalter, The Elevator, The One who lowers whoever He willed by His Destruction and raises whoever He willed by His Endowment",
  "@_NAME_NUMBER_24": {},
  "name_original_24": "ٱلْمُعِزُّ",
